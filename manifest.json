{
  "name": "Git Recommender",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Git recommender. See what others similar files people used",
  "homepage_url": "https://github.com/fuxes",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/browser_action/browser_action.html"
  }
}